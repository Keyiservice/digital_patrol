.custom-header-container {
  display: flex;
  align-items: center; /* 改为center实现垂直居中 */
  justify-content: center;
  padding: 50rpx 0 10rpx; /* 增加顶部内边距，将logo下移 */
  width: 100%;
  height: 220rpx; /* 增加高度以保持logo大小不变 */
  background-color: #0d3371;
  box-sizing: border-box;
  overflow: hidden; /* 新增：确保 aspectFill 裁剪的内容不溢出 */
}

.logo-image {
  width: 100%; /* 从 80% 恢复到 100% */
  height: 100%; /* logo高度占满容器 */
  object-fit: contain; /* 保持图片比例并完整显示 */
}

