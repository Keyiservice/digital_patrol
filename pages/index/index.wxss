/* index.wxss - 微信小程序样式表 */

/* 主容器 - 整个页面的布局容器 */
.container {
  min-height: 100vh;          /* 最小高度为视口高度，确保页面占满屏幕 */
  width: 100%; /* 使用100%而非100vw，更稳定 */
  display: flex;              /* 使用弹性布局 */
  flex-direction: column;     /* 垂直排列子元素 */
  justify-content: flex-start;/* 子元素从顶部开始排列 */
  padding: 40rpx 10rpx;       /* 页面整体左右间距减小 */
  box-sizing: border-box;     /* 包含边框和内边距在内的盒模型 */
  background-color: #f5f5f5;  /* 浅灰色背景 */
  /* overflow-x: hidden; */ /* 移除，防止宽度计算问题导致 */
}

/* 用户信息区域 */
.user-info {
  width: 100%;
  padding: 20rpx;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.welcome-text {
  font-size: 28rpx;
  color: #0d3371;
  font-weight: bold;
}

.content-section {
  width: 100%; /* 标题和按钮都用这个宽度 */
  margin: 0 auto;
}

/* 头部logo区域 - 顶部的蓝色横幅 */
.header {
  width: 90%;                 /* 宽度为父容器的90% */
  height: 120rpx;            /* 固定高度120rpx */
  background-color: #1e3a8a; /* 深蓝色背景 */
  border-radius: 20rpx;      /* 圆角边框 */
  display: flex;             /* 弹性布局 */
  align-items: center;       /* 垂直居中 */
  justify-content: center;   /* 水平居中 */
  margin-bottom: 60rpx;      /* 底部外边距60rpx */
}

/* logo图片样式 */
.logo {
  width: 200rpx;             /* logo宽度 */
  height: 60rpx;             /* logo高度 */
}

/* 标题区域 - 包含主标题的容器 */
.title-section {
  margin-bottom: 80rpx;      /* 减小底部外边距 */
  width: 100%;               /* 确保标题区域宽度 */
}

/* 主标题样式 */
.title {
  display: block;            /* 块级元素显示 */
  font-size: 80rpx;         /* 字体大小调整为50rpx */
  font-weight: bold;        /* 粗体字 */
  color: #0d3371;          /* 深蓝色文字 */
  text-align: center;      /* 文字居中对齐 */
  letter-spacing: 5rpx;    /* 字母间距调整为2rpx */
  text-shadow: 2rpx 4rpx 4rpx rgba(0,0,0,0.1); /* 文字阴影效果 */
  font-family: Arial, sans-serif;
  font-weight: bold;
  white-space: nowrap;
}

/* 网格布局容器 - 两列四行的按钮排列 */
.grid-container {
  width: 100%;
  display: grid;
  grid-template-columns: 1fr 1fr; /* 两列等宽 */
  grid-gap: 20rpx; /* 再缩小间距 (25rpx * 0.95 = 23.75rpx，取整为20rpx) */
  margin-bottom: 60rpx;
  box-sizing: border-box;
  padding: 0 20rpx; /* 进一步增加左右内边距，使更小的方块居中 */
}

/* 网格项目样式 */
.grid-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #0d3371;
  color: white;
  border-radius: 20rpx;
  box-shadow: 0 6rpx 12rpx rgba(30, 58, 138, 0.4);
  height: 216rpx; /* 再缩小5% (228rpx * 0.95) */
  padding: 18rpx; /* 再缩小5% (19rpx * 0.95) */
  box-sizing: border-box;
  transition: all 0.3s ease;
  transform: scale(0.9); /* 整体缩小到90% (原来的95%再缩小5%) */
  margin: 4rpx 0;
}

/* 网格项目点击效果 */
.grid-item:active {
  background-color: #0b2a5e;
  transform: translateY(2rpx);
}

/* 网格图标样式 */
.grid-icon {
  width: 108rpx; /* 再缩小5% (114rpx * 0.95) */
  height: 108rpx; /* 再缩小5% (114rpx * 0.95) */
  margin-bottom: 26rpx; /* 再缩小5% (28rpx * 0.95 = 26.6rpx，取整为26rpx) */
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  transition: transform 0.3s ease;
}

.grid-item:active .grid-icon {
  transform: scale(1.1);
}

/* 按钮内容样式 */
.btn-content {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  width: 100%;
  padding: 0 30rpx;
}

/* 菜单图标样式 */
.menu-icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
  flex-shrink: 0;
}

